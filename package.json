    "babel-watch": "^2.0.3",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "apidoc": "^0.16.1",
    "expect.js": "^0.2.0",
    "chai": "^3.5.0",
    "request": "^2.79.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "mongodb": "^2.2.11",
